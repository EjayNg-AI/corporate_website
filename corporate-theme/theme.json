{
  "$schema": "https://schemas.wp.org/wp/6.4/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "layout": {
      "contentSize": "700px",
      "wideSize": "1200px"
    },
    "spacing": {
      "blockGap": true,
      "padding": true,
      "margin": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"],
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "slug": "10",
          "size": "0.625rem",
          "name": "10"
        },
        {
          "slug": "20",
          "size": "1rem",
          "name": "20"
        },
        {
          "slug": "30",
          "size": "1.5rem",
          "name": "30"
        },
        {
          "slug": "40",
          "size": "2rem",
          "name": "40"
        },
        {
          "slug": "50",
          "size": "2.5rem",
          "name": "50"
        },
        {
          "slug": "60",
          "size": "4rem",
          "name": "60"
        },
        {
          "slug": "80",
          "size": "5rem",
          "name": "80"
        }
      ]
    },
    "typography": {
      "dropCap": false,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen-Sans, Ubuntu, Cantarell, \"Helvetica Neue\", sans-serif",
          "name": "System Font",
          "slug": "system-font"
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "0.875rem",
          "name": "Small",
          "fluid": {
            "min": "0.875rem",
            "max": "1rem"
          }
        },
        {
          "slug": "medium",
          "size": "1.125rem",
          "name": "Medium",
          "fluid": {
            "min": "1rem",
            "max": "1.125rem"
          }
        },
        {
          "slug": "large",
          "size": "1.25rem",
          "name": "Large",
          "fluid": {
            "min": "1.125rem",
            "max": "1.25rem"
          }
        },
        {
          "slug": "x-large",
          "size": "1.5rem",
          "name": "Extra Large",
          "fluid": {
            "min": "1.25rem",
            "max": "1.5rem"
          }
        },
        {
          "slug": "xx-large",
          "size": "2rem",
          "name": "XX Large",
          "fluid": {
            "min": "1.5rem",
            "max": "2rem"
          }
        },
        {
          "slug": "huge",
          "size": "2.8rem",
          "name": "Huge",
          "fluid": {
            "min": "2rem",
            "max": "2.8rem"
          }
        }
      ]
    },
    "color": {
      "defaultPalette": false,
      "defaultGradients": false,
      "defaultDuotone": false,
      "duotone": [],
      "gradients": [],
      "link": true,
      "custom": true,
      "customDuotone": false,
      "customGradient": false,
      "palette": [
        {
          "slug": "light-bg",
          "color": "#f5f6fa",
          "name": "Light Background"
        },
        {
          "slug": "light-bg-alt",
          "color": "#e1e3ea",
          "name": "Light Background Alt"
        },
        {
          "slug": "light-header",
          "color": "#ffffff",
          "name": "Light Header"
        },
        {
          "slug": "light-footer",
          "color": "#222222",
          "name": "Light Footer"
        },
        {
          "slug": "light-primary",
          "color": "#0057b8",
          "name": "Light Primary"
        },
        {
          "slug": "light-text",
          "color": "#111111",
          "name": "Light Text"
        },
        {
          "slug": "dark-bg",
          "color": "#23272f",
          "name": "Dark Background"
        },
        {
          "slug": "dark-bg-alt",
          "color": "#2e323a",
          "name": "Dark Background Alt"
        },
        {
          "slug": "dark-header",
          "color": "#181a20",
          "name": "Dark Header"
        },
        {
          "slug": "dark-primary",
          "color": "#4ea1ff",
          "name": "Dark Primary"
        },
        {
          "slug": "dark-text",
          "color": "#f5f6fa",
          "name": "Dark Text"
        },
        {
          "slug": "white",
          "color": "#ffffff",
          "name": "White"
        },
        {
          "slug": "black",
          "color": "#000000",
          "name": "Black"
        },
        {
          "slug": "transparent",
          "color": "transparent",
          "name": "Transparent"
        },
        {
          "slug": "light-gray",
          "color": "#e5e7eb",
          "name": "Light Gray"
        }
      ]
    },
    "blocks": {
    },
    "custom": {
      "spacing": {
        "small": "max(1rem, 2vw)",
        "medium": "max(1.5rem, 3vw)",
        "large": "max(2rem, 4vw)",
        "xlarge": "max(4rem, 8vw)"
      },
      "lineHeight": {
        "body": "1.7",
        "heading": "1.2"
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--light-bg)",
      "text": "var(--wp--preset--color--light-text)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--system-font)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.7"
    },
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--30)",
      "padding": {
        "top": "0",
        "right": "0",
        "bottom": "0",
        "left": "0"
      }
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--light-primary)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--light-primary)"
          },
          "typography": {
            "textDecoration": "underline"
          }
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--line-height--heading)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--line-height--heading)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--line-height--heading)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--line-height--heading)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--line-height--heading)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--line-height--heading)"
        }
      },
      "button": {
        "border": {
          "radius": "30px"
        },
        "color": {
          "background": "var(--wp--preset--color--light-primary)",
          "text": "var(--wp--preset--color--white)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600"
        },
        "spacing": {
          "padding": {
            "top": "12px",
            "right": "28px",
            "bottom": "12px",
            "left": "28px"
          }
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--light-primary)",
            "text": "var(--wp--preset--color--white)"
          }
        }
      }
    },
    "blocks": {
      "core/button": {
        "variations": {
          "outline": {
            "spacing": {
              "padding": {
                "top": "12px",
                "right": "28px",
                "bottom": "12px",
                "left": "28px"
              }
            },
            "border": {
              "width": "2px"
            }
          }
        }
      },
      "core/navigation": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        },
        "elements": {
          "link": {
            ":hover": {
              "color": {
                "text": "var(--wp--preset--color--light-primary)"
              }
            }
          }
        }
      },
      "core/site-title": {
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--light-text)"
            },
            ":hover": {
              "typography": {
                "textDecoration": "none"
              }
            }
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600"
        }
      },
      "core/post-title": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            }
          }
        }
      },
      "core/comment-reply-link": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/post-comments-form": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    },
    {
      "name": "post-meta",
      "title": "Post Meta",
      "area": "uncategorized"
    }
  ],
  "customTemplates": [
    {
      "name": "blank",
      "title": "Blank",
      "postTypes": ["page", "post"]
    },
    {
      "name": "page-no-title",
      "title": "Page (No Title)",
      "postTypes": ["page"]
    }
  ]
}